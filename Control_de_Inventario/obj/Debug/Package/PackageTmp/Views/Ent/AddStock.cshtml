@model Control_de_Inventario.Models.vwmostrarentrada
@{
    Layout = null;
}

@using (Html.BeginForm("AddStock", "Ent", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.id_articulo)
    <div class="form-group">
        @Html.LabelFor(model => model.inv_cantidad, "", new { @class = "control-label" })
        @Html.EditorFor(model => model.inv_cantidad, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.inv_cantidad)
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary" />
        <input type="reset" value="Limpiar" class="btn" />
    </div>
}
